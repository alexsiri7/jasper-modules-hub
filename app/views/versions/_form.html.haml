= simple_form_for([@plugin, @version]) do |f|
  = f.error_notification

  .form-inputs
    = f.input :number
    = f.input :jasper_version
    = f.input :file_type, collection: Version::types_as_select, include_blank: false
    = f.input :file
    = f.input :notes

  .form-actions
    = f.button :submit
